<% 
var upDate = new Date(Number(update));
var yyyy = upDate.getFullYear() + '';
var mm = upDate.getMonth() + 1;
    mm = '0' + mm;
    mm = mm.slice(-2);
var dd = '0' + upDate.getDate();
    dd = dd.slice(-2);
%>
<div>
    <div class="pubDate">
        <span class="icon-list">
            <% if (notebook === '技術') { %>
            <i class="fa fa-globe" aria-hidden="true"></i>
            <% } else if (notebook === 'ラーメン') { %>
            <i class="fa fa-cutlery" aria-hidden="true"></i>
            <% } else if (notebook === 'ブログ-マネジメント') { %>
            <i class="fa fa-user" aria-hidden="true"></i>
            <% } %>
        </span>
        <%= yyyy %>年<%= mm %>月<%= dd %>日 更新しました</div>
    <h1><%= noteTitle %></h1>
    <div class="share-btn-box">
        <div class="fb-like" data-href="https://sssatoru-t.info/blog/<%= update %>" data-layout="button_count" data-action="like" data-size="small" data-show-faces="false" data-share="false"></div>
        <div class="tw-like">
            <a href="https://twitter.com/share" class="twitter-share-button" data-hashtags="ブログ">Tweet</a> <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>
        </div>
    </div>
    <div class="content-wrap"><%- noteText %></div>
</div>
